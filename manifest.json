{
    "manifest_version": 3,
    "name": "Patient Mapper",
    "version": "2.0",
    "description": "Maps hospital patients.",
    "permissions": ["tabs", "scripting", "activeTab"],
    "host_permissions": ["https://api.rsudsoediranms.com/*"],
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
